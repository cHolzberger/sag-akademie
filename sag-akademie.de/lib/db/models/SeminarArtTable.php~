class SeminarArtTable extends Doctrine_Table
{
 public static function detailed($id="?") {
	$q = Doctrine_Query::create()
    ->from('SeminarArt a')
    ->leftJoin('a.GeaendertVon updater')
    ->leftJoin('a.Seminare b')
    	->leftJoin('b.Standort e')
    		->leftJoin('e.Ort f')
    ->leftJoin('a.Rubrik d')
    ->leftJoin('a.Status g');
	if ( $id != "?" ) $q->where('a.id = ?', $id);
	else $q->where("a.id = ?");

	return $q;
 }
  public static function detailedList() {
	$q = Doctrine_Query::create()
	->select ("a.*, d.name, g.name")
	  ->from('SeminarArt a')
	 ->leftJoin('a.Rubrik d')
	  ->leftJoin('a.Status g');
	

	return $q;
 }
