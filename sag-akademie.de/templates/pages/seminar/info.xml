	<hinweis></hinweis>

	<!--<image key="title" style="position: absolute; left:0px; top:0px;" />-->
	<div style="text-align:right;padding-bottom:3px;width:840px;" class="noprint">
		<a href="javascript:print();" class="schwarz">
			<b>Seite drucken</b>
		</a> &nbsp;I&nbsp; 
		<a href="javascript:history.back();" class="schwarz">
			<b>zurück zur Übersicht</b>
		</a>
	</div>
	<div style="position: relative; width:950px; margin-top:0px;">
		<div style="width: 500px; padding-left: 10px;" class="seminartext doprinttext">
			<span style="color:#666666 !important;">
				<b>
					<mdb:value mpath="Rubrik:name" />&nbsp;&nbsp;
					<span style="color:#000000;">&bull;</span>&nbsp;
					<dbfield name="id" notable="1" />&nbsp;&nbsp;
					<span style="color:#000000;">&bull;</span>&nbsp;
					<dbfield name="bezeichnung" notable="1" />
				</b>
			</span>
			<br/>
			<b>
				<span style="font-size:12px !important;">
					<dbfield name="kurzbeschreibung" class="kurzbeschreibung" notable="1" />
				</span>
			</b>
			<br />&nbsp;
			<br />
			<span style="color:#666666 !important;">
				<b>Allgemeines</b>
			</span>
			<br/>
			<img src="/img/trennerlinks.png" alt="" border="0" width="520" height="1" style="margin-bottom:4px;"/>
			<br />
			<dbfield name="seminar_hinweis" />
			<br />
			<span style="color:#666666 !important;">
				<b>Zielgruppe</b>
			</span>
			<br/>
			<img src="/img/trennerlinks.png" alt="" border="0" width="520" height="1" style="margin-bottom:4px;"/>
			<br />
			<dbfield name="zielgruppe" />
			<br />
			<span style="color:#666666 !important;">
				<b>Teilnahme- und Zulassungsvoraussetzungen</b>
			</span>
			<br/>
			<img src="/img/trennerlinks.png" alt="" border="0" width="520" height="1" style="margin-bottom:4px;"/>
			<br />
			<dbfield name="voraussetzungen" />
			<br />
			<span style="color:#666666 !important;">
				<b>Seminarbeschreibung / Seminarinhalte</b>
			</span>
			<br/>
			<img src="/img/trennerlinks.png" alt="" border="0" width="520" height="1" style="margin-bottom:4px;"/>
			<br />
			<dbfield name="langbeschreibung" />
			<br />
			<span style="color:#666666 !important;">
				<b>Lehrgangszertifikate / Teilnahmebescheinigungen</b>
			</span>
			<br/>
			<img src="/img/trennerlinks.png" alt="" border="0" width="520" height="1" style="margin-bottom:4px;"/>
			<br />
			<dbfield name="nachweise" />
			<br />
			<span style="color:#666666 !important;">
				<b>Kooperationen</b>
			</span>
			<br/>
			<img src="/img/trennerlinks.png" alt="" border="0" width="520" height="1" style="margin-bottom:4px;"/>
			<br />
			<foreach fromArray="Kooperationspartner">
				<mdb:link href="#DATA#" mpath="link" urlencode="false" target="_blank">
					<mdb:image mpath="logo" noImgLink="true" noPadding="true" />
				</mdb:link>&nbsp;

			</foreach>
		</div>
		<div style="position: absolute; top: 0px; left: 580px;" class="doprint">&nbsp;
			<br />
			<div style="padding:5px;">
				<b>
					<span style="color:#666666 !important;">Kosten:</span>
				</b>&nbsp;
				<dbfield name="preis" converter="euroPreis" notable="1" suffix="&nbsp;€&nbsp;zzgl. MwSt." />
				<br/>
				<b>
					<span style="color:#666666 !important;">Dauer:</span>
				</b>&nbsp;&nbsp;&nbsp;&nbsp;
				<dbfield name="dauer" notable="true" />&nbsp;
				<switch key="dauer">
					<case value="1">Tag</case>
					<case value="*">Tage</case>
				</switch>
				<br/>
				<br/>
				<div>
				<mdb:link href="/pdf/top2;pdf/#DATA#" mpath="id" target="_blank">Seminar-Infos
					<img src="https://www.sag-akademie.de/files/pdf_klein.png" border="0" alt="" style="position:relative; top:3px;"/>
				</mdb:link>
				</div>
				<br/>
				<br/>				
				<b>
					<span style="color:#666666 !important;">Termine und Buchung</span>
				</b>
				<br />
				<span class="noprint">Klicken Sie auf einen Termin um diesen zu buchen
					<br/>&nbsp;
					<br />
				</span>
				<foreach fromArray="Seminare" basehref="/seminar/buchen/" linkId="id" onEmpty="Termine auf Anfrage">
					<switch key="ausgebucht">
						<case value="0">
							<div style="float: left; width: 80px;">
								<dbfield name="datum_begin" converter="mysqlDateToLocal"/>
							</div>
							<dbfield name="Standort:Ort:ort" />&nbsp;
					(
							<dbfield name="kursnr" />)
					
							<switch key="storno_datum">
								<!--  if else abfrage -->
								<case value="0000-00-00"></case>
								<case value="*">
							&nbsp;&nbsp;
									<span style="color: red;">Abgesagt!</span>
								</case>
							</switch>
							<br/>
						</case>
						<case value="*">
							<div style="float: left; width: 80px;">
								<dbfield name="datum_begin" converter="mysqlDateToLocal"/>&nbsp;&nbsp;&nbsp;
					
							</div>
							<dbfield name="Standort:Ort:ort" />&nbsp;&nbsp;
							<span style="color: red;">Ausgebucht!</span>
							<br/>
						</case>
					</switch>
				</foreach>
			</div>
			<br />
			<div class="noprint">
				<sag:tops></sag:tops>
			</div>
		</div>
		<div style="clear: both;">&nbsp;</div>
	</div>
</content>
