              <dbfield name="Seminar:datum_begin" converter="mysqlDateToLocal" notable="1" />-
              <dbfield name="Seminar:datum_ende" converter="mysqlDateToLocal" notable="1" /></span>
            </p>
          </td>
        </tr>
        <tr>
          <td style="padding:2.25pt 2.25pt 2.25pt 2.25pt;">
            <p class="MsoNormal" style="margin-bottom:.0001pt;">
              <span style="font-size:9pt;">Ort</span>
            </p>
          </td>
          <td width="494" style="width:370.75pt;padding:2.25pt 2.25pt 2.25pt 2.25pt;">
            <p class="MsoNormal" style="margin-bottom:.0001pt;">
              <span style="font-size:9pt;">
                <dbfield name="Seminar:Standort:Ort:ort" notable="1" />
              </span>
            </p>
          </td>
        </tr>
        <tr>
          <td style="padding:2.25pt 2.25pt 2.25pt 2.25pt;">
            <p class="MsoNormal" style="margin-bottom:.0001pt;">
              <span style="font-size:9pt;">Kosten (zzgl. MwSt.)</span>
            </p>
          </td>
          <td width="494" style="width:370.75pt;padding:2.25pt 2.25pt 2.25pt 2.25pt;">
